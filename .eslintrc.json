{
  "extends": ["next/core-web-vitals", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "warn",
      {
        "singleQuote": false,
        "semi": true,
        "trailingComma": "es5",
        "tabWidth": 2,
        "printWidth": 80,
        "endOfLine": "auto"
      }
    ]
  }
}
